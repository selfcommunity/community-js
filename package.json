{
  "name": "@selfcommunity/community-ui",
  "private": true,
  "scripts": {
    "bootstrap": "yarn && lerna bootstrap --use-workspaces",
    "build": "lerna run build",
    "prerelease": "lerna prerelease",
    "release": "lerna exec publish",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "tdd": "npm run rw tdd",
    "clean:node_modules": "lerna exec yarn clean:node_modules && rimraf node_modules",
    "clean:libs": "lerna exec yarn clean:lib",
    "docs:start": "yarn --cwd ./docs start",
    "docs:deploy": "yarn build && yarn --cwd ./docs && GIT_USER=selfcommunity yarn --cwd ./docs deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "SelfCommunity (https://www.selfcommunity.com)",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-modules": "^0.1.1",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/react": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-preset-env-modules": "^1.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^7.0.0",
    "eslint-config-jason": "^7.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.16.0",
    "glob": "^7.1.3",
    "lerna": "^4.0.0",
    "lodash": "^4.17.11",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selfcommunity/community-ui"
  }
}
